<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
	<metadata>
		<id>nloptnet</id>
		<version>1.4.1</version>
		<authors>Brannon King</authors>
		<description>This is a wrapper around the NLopt library. Binaries are included.</description>
		<releaseNotes>Reducing installation warnings.</releaseNotes>
		<tags>NLopt NLoptNet Optimization Minimization Simplex Stogo Cobyla BFGS</tags>
		<repository type="git" url="https://github.com/BrannonKing/NLoptNet"></repository>
		<dependencies>
			<group targetFramework=".NETFramework4.7" />
			<group targetFramework=".NETStandard2.0" />
		</dependencies>
	</metadata>
	<files>
		<file src="**" exclude="**\obj\**;**\bin\**;**\Properties\**;*.cs;*.csproj" />
		<file src="bin\AnyCPU\Release\net47\NLoptNet.dll" target="lib\net47" />
		<file src="bin\AnyCPU\Release\netstandard2.0\NLoptNet.dll" target="lib\netstandard2.0" />
	</files>
</package>